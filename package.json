{
  "name": "built-in-public",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "prepare": "husky",
    "lint": "next lint",
    "precheck": "bash ./scripts/precheck.sh",
    "semgrep": "semgrep scan --config p/default --config p/owasp-top-ten --config security/semgrep/general.yml --exclude supabase/seed.sql --exclude .env.example --metrics=off --error",
    "test": "vitest --exclude **/rls-policies",
    "test:rls": "vitest /rls-policies",
    "test:watch": "vitest --watch",
    "db:migrate": "supabase db reset",
    "db:seed": "tsx scripts/seed.ts",
    "db:setup": "npm run db:migrate && npm run db:seed",
    "start:dev": "npm run db:setup && npm run dev",
    "generate:types": "tsx scripts/generateSupabaseTypes.ts"
  },
  "dependencies": {
