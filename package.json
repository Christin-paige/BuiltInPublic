    "emoji-regex": "^10.5.0",
    "leo-profanity": "^1.8.0",
    "lucide-react": "^0.542.0",
    "next": "^15.5.2",
    "next-themes": "^0.4.6",
    "posthog-js": "^1.261.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-github-calendar": "^4.5.9",
    "react-hook-form": "^7.62.0",
    "react-icons": "^5.5.0",
    "react-toastify": "^11.0.5",
