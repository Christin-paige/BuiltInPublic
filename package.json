    "@vitejs/plugin-react": "^5.0.2",
    "eslint": "^9.34.0",
    "eslint-config-next": "15.5.2",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "supabase": "^2.39.2",
